{
  "name": "@yimicat/eslint-config",
  "type": "module",
  "version": "0.1.0",
  "description": "eslint 配置",
  "publishConfig": {
    "access": "public"
  },
  "author": "Yimi",
  "license": "MIT",
  "homepage": "https://github.com/yimicat/eslint-config/tree/readme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yimicat/eslint-config.git#readme"
  },
  "bugs": {
    "url": "https://github.com/yimicat/eslint-config/issues"
  },
  "keywords": [
    "eslint"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "bump package.json packages/*/package.json",
    "lint:eslint": "eslint . --fix",
    "prepare": "husky",
    "cz": "czg",
    "log": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "build": "unbuild"
  },
  "dependencies": {
    "@yimicat/commitlint-config": "workspace:*",
    "@yimicat/eslint-config-base": "workspace:*",
    "@yimicat/eslint-config-prettier": "workspace:*"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "commitlint": "^19.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "czg": "^1.9.3",
    "eslint": "^9.7.0",
    "husky": "^9.0.11",
    "typescript": "^5.5.3",
    "unbuild": "^v3.0.0-rc.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
